  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prajitdas/prajitdas.github.io/issues"
  },
  "homepage": "https://prajitdas.github.io/",
  "devDependencies": {
    "jslint": "^0.12.1",
    "live-server": "^1.2.2",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
