  "description": "Dr. Prajit Kumar Das - Personal Website with JSLint Testing",
  "main": "index.html",
  "scripts": {
    "lint": "jslint assets/js/*.js",
    "lint:verbose": "jslint assets/js/*.js --verbose",
    "lint:main": "jslint assets/js/main.js",
    "lint:custom": "jslint assets/js/custom.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint",
    "dev": "live-server --port=8080 --host=localhost --open=/",
    "build": "npm run lint && echo 'Build successful - all JS files pass JSLint'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prajitdas/prajitdas.github.io.git"
  },
